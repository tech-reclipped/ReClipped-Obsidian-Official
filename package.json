{
  "name": "reclipped-obsidian-official",
  "version": "1.0.0",
  "description": "Automatically syncs your ReClipped annotations with Obsidian.",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js --environment RECLIPPED_SERVER_URL:https://reclipped.com",
    "dist": "npm run build && mkdir -p dist && cp styles.css main.js manifest.json dist"
  },
  "keywords": [
    "obsidian",
    "reclipped"
  ],
  "author": "ReClipped",
  "license": "MIT",
  "dependencies": {
    "open": "^7.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-player": "^2.10.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/node": "^16.11.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "dotenv": "^10.0.0",
    "esbuild": "0.13.12",
    "obsidian": "latest",
    "rollup": "^2.32.1",
    "rollup-plugin-dotenv": "^0.3.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  }
}
