{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "es6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "jsx": "react",
	"sourceMap": false,
    "lib": [
      "dom",
      "es5",
      "ES6",
      "ES7",
      "scripthost",
      "es2015"
    ]
  },
  "include": [
    "**/*.ts",
    "view/VideoView.tsx",
    "view/VideoContainer.tsx",
  ]
}
